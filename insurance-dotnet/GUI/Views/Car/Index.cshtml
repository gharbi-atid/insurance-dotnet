@model IEnumerable<Data.Models.car>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_insurancetemplate.cshtml";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idCar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contract_refContract)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.marque)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.matricule)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.modele)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idCar)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contract_refContract)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.marque)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matricule)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.modele)
        </td>
    </tr>
}

</table>
